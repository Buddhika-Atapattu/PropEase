{
  "name": "propease",
  "version": "0.0.0",
  "private": true,
  "main": "dist-electron/electron-main.js",
  "scripts": {
    "ng": "ng",
    "start": "npm run electron-dev",
    "dev": "ng serve",
    "compile:electron": "tsc -p tsconfig.electron.json",
    "watch:electron": "tsc -p tsconfig.electron.json --watch",
    "electron-dev": "concurrently \"npm run watch:electron\" \"ng serve\" \"wait-on http://localhost:4200 && electron .\"",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint"
  },
  "dependencies": {
    "@angular/animations": "^20.0.2",
    "@angular/cdk": "^20.0.2",
    "@angular/common": "^20.0.2",
    "@angular/compiler": "^20.0.2",
    "@angular/core": "^20.0.2",
    "@angular/forms": "^20.0.2",
    "@angular/material": "^20.0.2",
    "@angular/material-moment-adapter": "^20.0.2",
    "@angular/platform-browser": "^20.0.2",
    "@angular/platform-browser-dynamic": "^20.0.2",
    "@angular/platform-server": "^20.0.2",
    "@angular/router": "^20.0.2",
    "@angular/service-worker": "^20.0.2",
    "@angular/ssr": "^20.0.1",
    "@popperjs/core": "^2.11.8",
    "@tinymce/tinymce-angular": "^8.0.1",
    "@yusifaliyevpro/countries": "^2.2.2",
    "abort-controller": "^3.0.0",
    "adbkit": "^2.0.17",
    "angular-google-charts": "^16.1.0",
    "bootstrap": "^5.3.3",
    "d3": "^7.9.0",
    "dotenv": "^16.5.0",
    "drivelist": "^12.0.2",
    "express": "^4.18.2",
    "file-saver": "^2.0.5",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "leaflet": "^1.9.4",
    "leaflet-geosearch": "^4.2.0",
    "libphonenumber-js": "^1.12.9",
    "moment": "^2.30.1",
    "ngx-image-cropper": "^9.1.5",
    "noble": "^0.0.2",
    "node-twain": "^0.0.16",
    "oui": "^13.0.5",
    "ping": "^0.4.4",
    "qrcode": "^1.5.4",
    "quill": "^2.0.3",
    "rxjs": "~7.8.0",
    "sharp": "^0.34.2",
    "socket.io-client": "^4.8.1",
    "tesseract.js": "^6.0.1",
    "tslib": "^2.3.0",
    "usb": "^2.15.0",
    "usb-detection": "^4.14.2",
    "xlsx": "^0.18.5",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@analogjs/vite-plugin-angular": "^1.16.0",
    "@angular-devkit/build-angular": "^20.0.2",
    "@angular/build": "^20.0.1",
    "@angular/cli": "^20.0.1",
    "@angular/compiler-cli": "^20.0.2",
    "@angular/localize": "^20.0.2",
    "@types/ckeditor__ckeditor5-build-classic": "^29.0.1",
    "@types/dotenv": "^6.1.1",
    "@types/drivelist": "^6.4.4",
    "@types/express": "^4.17.17",
    "@types/file-saver": "^2.0.7",
    "@types/google.maps": "^3.58.1",
    "@types/jasmine": "~5.1.0",
    "@types/leaflet": "^1.9.18",
    "@types/multer": "^1.4.13",
    "@types/ping": "^0.4.4",
    "@types/qrcode": "^1.5.5",
    "@types/sharp": "^0.31.1",
    "concurrently": "^9.2.0",
    "electron": "^37.1.0",
    "electron-builder": "^26.0.12",
    "electron-rebuild": "^3.2.9",
    "electron-reload": "^2.0.0-alpha.1",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "wait-on": "^8.0.3"
  },
  "build": {
    "appId": "com.propease.app",
    "productName": "PropEase",
    "directories": {
      "buildResources": "public"
    },
    "files": [
      "dist/",
      "dist-electron/",
      "public/**/*",
      "electron-main.js",
      "electron/**/*"
    ],
    "extraMetadata": {
      "main": "dist-electron/electron-main.js"
    },
    "win": {
      "icon": "public/Images/company-images/ICO/PropEaseLogo.ico",
      "target": [
        "nsis"
      ]
    },
    "mac": {
      "icon": "public/Images/company-images/ICNS/PropEaseLogo1024X1024.icns",
      "target": [
        "dmg"
      ]
    },
    "linux": {
      "icon": "public/Images/company-images/PropEaseLogo1024X1024.png",
      "target": [
        "AppImage",
        "deb"
      ]
    }
  }
}
